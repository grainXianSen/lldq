/* components/banner/banner.acss */
.banner {
    position: relative;
    width: 100%;
    height: 350rpx;
    overflow: hidden;
}
.banner-image {
    min-width: 100%;
}
.banner-desc {
    position: absolute;
    left: 100rpx;
    top: 80rpx;
    padding-right: 40rpx;
    color: #333;
}
.banner-desc .head {
    margin-bottom: 10rpx;
    font-size: 36rpx;
    font-weight: bold;
}
.banner-desc .desc {
    font-size: 28rpx;
}